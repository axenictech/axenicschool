<div id="content-header">
  <h1>Exam Reports</h1>
  <h3>Ranking Level Report</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0), :back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>
<div id="page-yield">
  <div class="box">
<div class="bread_crumb">
    <%= link_to 'Exams', '/exams' %> &raquo;
    <%= link_to 'Report Center', '/exam_reports/report_center' %> &raquo;
    Ranking Level Report
  </div>
   <% unless flash[:notice].nil? %>
    <div class="alert alert-success" style="width:50%"> <%= flash[:notice] %> </div>
    <%end %>
	<%= form_for :mode, :url=>{:action=>'select_mode'},html:{id: 'form_mode'},remote:true,method: :get do |f| %>
	  
	<div class="label-field-pair">
	  <%= f.label :select_mode %>
	      <div class="text-input-bg">
	      <%= f.select :wise, ["Course wise","Batch wise"],{prompt:true},{onchange:"$('#form_mode').submit();"}%>
	      </div></div>
	<%end%>
<div id="mode_wise_view"></div>
</div></div>