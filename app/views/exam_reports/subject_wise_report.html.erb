<div id="content-header">
  <h1>Exam Reports</h1>
  <h3>Subject Wise Report</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0), :back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>
<div id="page-yield">

  
  <div class="box">

  <div class="bread_crumb">
    <%= link_to 'Exams', '/exams' %> &raquo;
    <%= link_to 'Report Center', '/exam_reports/report_center' %> &raquo;
    Subject Wise Report
  </div>
   <% unless flash[:notice_s_r].nil? %>
    <div class="alert alert-danger" align="center" style="width:100%"> <%= flash[:notice_s_r] %> </div>
    <%end %>
<%= form_for :batch_choose, :url=>{:action=>'choose_batch'},html:{id: 'form_batch'},remote:true,method: :get do |f| %>
  
 <div class="label-field-pair">
  <%= f.label :select_batch %>
      <div class="text-input-bg">
      <%= f.select :id, @batches.collect {|b| [b.course.course_name+" "+b.course.section_name+" - "+b.name,b.id]},{prompt:true},{onchange:"$('#form_batch').submit();"}%>
      </div></div>
  <% end %>
      <div id="subject_report"></div>
  </div>
</div>