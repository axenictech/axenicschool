<div id="content-header">
  <h1>Exams</h1>
  <h3>Grading levels</h3>
  <div id="app-back-button">
      <%= link_to image_tag("buttons/back.png",:border => 0), :back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="inner-tab-menu">
  <ul>
    <li><%= link_to 'Courses', courses_path %></li>
  </ul>
</div>
<div id="page-yield">

    <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>

 <div class="box">
<div class="text-input-bg">
<%= form_for :batch, url:{action:'select'},method: :get,remote:true,html:{id: "batch_form"} do |f| %>
<%= f.label :select_batch %>
<%= f.select :id, @batches.collect {|b| [b.course.course_name+" "+b.course.section_name+" - "+b.name, b.id]},{prompt:true},{onchange:"$('#batch_form').submit();"} %>  
<%end%>
</div><br/><br/><br/>

<div id="batch_div"></div>
 <div id="myModal" class="modal fade">
 </div>
</div></div>



