
 <div class="col-md-1"></div>
      <table align="center" class="table table-hover" style="width:1000px">
      <tr>
            <th>Employee Name</th>
            <th>Employee Id</th>
            <th>Employee Deprtment</th>
      </tr>

      <%@employees.each do |emp|%>
          <tr>
                <td col-md-1><%= link_to emp.first_name,employees_profile_path(emp)%></td>
                <td col-md-1><%= emp.employee_number%></td>
                <td col-md-1><%= emp.employee_department.name%></td>
         </tr>
      <%end%>  
      </table>
  
 
