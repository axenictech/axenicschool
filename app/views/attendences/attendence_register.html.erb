<div id="content-header">
  <h1>Attendance</h1>
  <h3>Attendance register</h3>
  <div id="app-back-button">
      <%= link_to image_tag("buttons/back.png",:border => 0), :back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>


<div id="page-yield">
 <div class="box">
<div class="text-input-bg">
<%= form_for :batch, url:{action:'select_subject'},method: :get,remote:true ,html:{id: "course_form"} do |f| %>
<%= f.label :select_batch %>
<%= f.select :batch_id, Batch.all.collect {|b| [b.course.code+" - "+b.name,b.id]},{prompt:true},{onchange:"$('#course_form').submit();"}%>
<%end%>
</div><br/><br/><br/>
 <div id="subject_div"></div></div>
<div id="myModal" class="modal fade">
 </div>


