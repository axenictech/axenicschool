<div id="content-header">
  <h1>All Users</h1>
  <h3>Details about  all users</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0),:back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="page-yield">

  <% unless flash[:notice].nil? %><p class="flash-msg"> <%= flash[:notice] %> </p><% end %>
<div id="box">
  <div class="text-input-bg">
		<%= form_for :user, url:{action:'select'},method: :get,remote:true,html:{id: 'user_form2'} do |f| %>
		<%= f.label :select %>
	    <%= f.select :role,["Admin", "Employee", "Student"],{prompt:true},{onchange:"$('#user_form2').submit();"} %>
		<% end %>
	</div></div><br/><br/>
 <div id="user_view"></div> 
</div>
